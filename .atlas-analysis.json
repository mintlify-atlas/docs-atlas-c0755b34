  "keyFeatures": [
    "Microkernel architecture with dual-layer plugin system",
    "Rust core with UniFFI multi-language bindings",
    "Runtime programmability via Rhai scripting",
    "Multi-agent collaboration patterns (request-response, pub-sub, consensus, debate, parallel, sequential)",
    "LLM provider abstraction (OpenAI, Anthropic, Ollama, Gemini)",
    "ReAct pattern for reasoning and action",
    "Secretary agent mode for human-in-the-loop workflows",
    "Workflow orchestration with StateGraph API",
    "Distributed dataflow via Dora-rs integration",
    "Persistence layer supporting PostgreSQL, MySQL, SQLite",
    "Hot-reloadable plugins and scripts",
    "CLI tool for scaffolding and agent management"
  ],
  "publicApiSurface": [
    "MoFAAgent trait - Core agent interface",
    "AgentBuilder - Fluent API for constructing agents",
    "LLMProvider trait - LLM integration interface",
    "LLMClient - High-level LLM interaction client",
    "Tool trait - Tool/function calling interface",
    "AgentPlugin trait - Plugin system interface",
    "StateGraph API - Workflow orchestration",
    "SimpleRuntime - Multi-agent coordination runtime",
    "AgentRuntime - Dora-rs distributed runtime",
    "ReActAgent - Reasoning and action agent",
    "SecretaryAgent - Human-in-the-loop agent",
    "CoordinationPattern - Multi-agent collaboration patterns",
    "RhaiPlugin - Runtime scripting plugin",
    "ToolPlugin - Tool registration and execution",
    "PersistencePlugin - Session and message persistence",
    "MessageBus - Agent communication bus",
    "mofa CLI - Agent scaffolding and management commands"
  ]
}
